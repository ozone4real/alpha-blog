<%= form_with scope: :article, url: articles_path, local: true do |form| %>
<h1> Create an article </h1>
<div>
<%= form.label :title %> <br/>
<%= form.text_field :title %>
</div>
<div>
<%= form.label :description %><br/>
<%= form.text_area :description %>
</div>
<div>
<%= form.submit %> 
</div>
<% end %>